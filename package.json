    "@tailwindcss/line-clamp": "^0.2.1",
    "@types/parse-numeric-range": "0.0.1",
    "@types/styled-jsx": "^2.2.8",
    "got": "^11.8.2",
    "gray-matter": "^4.0.3",
    "ignore-loader": "^0.1.2",
    "metascraper": "^5.22.5",
    "metascraper-author": "^5.22.5",
    "metascraper-clearbit": "^5.23.1",
    "metascraper-date": "^5.22.5",
    "metascraper-description": "^5.22.5",
    "metascraper-image": "^5.23.1",
    "metascraper-logo": "^5.25.7",
    "metascraper-logo-favicon": "^5.22.5",
    "metascraper-publisher": "^5.22.5",
    "metascraper-title": "^5.23.1",
    "metascraper-url": "^5.22.5",
    "next": "11.0.1",
    "next-absolute-url": "^1.2.2",
    "next-mdx-remote": "^3.0.4",
    "next-seo": "^4.26.0",
    "parse-numeric-range": "^1.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-unwrap-images": "^2.1.0",
    "swr": "^0.5.6",
    "trim": "^1.0.1"
  },
  "devDependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.3",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/glob": "^7.1.3",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.12",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^2.2.4",
    "textlint": "^12.0.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-filter-rule-whitelist": "^2.0.0",
    "textlint-rule-ja-hiragana-fukushi": "^1.3.0",
